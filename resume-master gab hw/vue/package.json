{
  "name": "vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2",
    "vue": "^3",
    "vue-router": "^4",
    "vue3-mq": "^3"
  },
  "devDependencies": {
    "@peterek/vite-plugin-favicons": "^1",
    "@rushstack/eslint-patch": "^1",
    "@tsconfig/node18": "^18",
    "@types/node": "^18",
    "@vitejs/plugin-vue": "^4",
    "@vue/eslint-config-prettier": "^8",
    "@vue/eslint-config-typescript": "^12",
    "@vue/tsconfig": "^0",
    "autoprefixer": "^10",
    "daisyui": "^3",
    "eslint": "^8",
    "eslint-plugin-vue": "^9",
    "npm-run-all2": "^6",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3",
    "typescript": "~5.2.0",
    "vite": "^4",
    "vue-tsc": "^1"
  }
}
